<p id="notice"><%= notice %></p>

<h1>Bankdetails</h1>

<table>
  <thead>
    <tr>
      <th>Account name</th>
      <th>Country</th>
      <th>Iban</th>
      <th>Bic</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bankdetails.each do |bankdetail| %>
      <tr>
        <td><%= bankdetail.account_name %></td>
        <td><%= bankdetail.country %></td>
        <td><%= bankdetail.iban %></td>
        <td><%= bankdetail.bic %></td>
        <td><%= bankdetail.user_id %></td>
        <td><%= link_to 'Show', bankdetail %></td>
        <td><%= link_to 'Edit', edit_bankdetail_path(bankdetail) %></td>
        <td><%= link_to 'Destroy', bankdetail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bankdetail', new_bankdetail_path %>
